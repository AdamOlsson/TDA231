    "# Mu according to MLE\n",
    "def mean(data):\n",
    "    return sum(data)/len(data)\n",
    "\n",
    "# Variance according to MLE\n",
    "def variance(data, mu):\n",
    "    return np.sum(np.square(data-mu))/len(data)\n",
